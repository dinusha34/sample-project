<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">



	<servlet>
		<servlet-name>EjnarMotorShop</servlet-name>
		<servlet-class>web.EjnarMotorShopServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EjnarMotorShop</servlet-name>
		<url-pattern>/ejnarmotorshop</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>web.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoggedUser</servlet-name>
		<servlet-class>web.LoggedUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoggedUser</servlet-name>
		<url-pattern>/loggeduser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RegisterUser</servlet-name>
		<servlet-class>web.RegisterUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterUser</servlet-name>
		<url-pattern>/registeruser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GoogleLogin</servlet-name>
		<servlet-class>web.GoogleLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoogleLogin</servlet-name>
		<url-pattern>/googlelogin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>web.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Chat</servlet-name>
		<servlet-class>web.Chat</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Chat</servlet-name>
		<url-pattern>/chat</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ChatAdmin</servlet-name>
		<servlet-class>web.ChatAdmin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChatAdmin</servlet-name>
		<url-pattern>/chatadmin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ChatData</servlet-name>
		<servlet-class>web.ChatData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChatData</servlet-name>
		<url-pattern>/chatdata</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CustomizedCar</servlet-name>
		<servlet-class>web.CustomizedCar</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CustomizedCar</servlet-name>
		<url-pattern>/customizedcar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddCar</servlet-name>
		<servlet-class>web.AddCar</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddCar</servlet-name>
		<url-pattern>/addcar</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
